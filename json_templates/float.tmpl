// {{- /* Go Template */ -}}.
{{- define "float" -}}
    {{- $header := "\"," -}}
    {{- $end := "end" -}}
    {{- if eq .Start "firststart" -}}
        {{- $end = "firstend" -}}
        {{- $header = "\"{" -}}
    {{- else if eq .PreviousType "string" -}}
        {{- $header = "\"\\\"," -}}
    {{- else if eq .PreviousType "[]string" -}}
        {{- $header = "\"\\\"]," -}}
    {{- else if (and (eq .PreviousArray "true") (not (eq .PreviousType "[]bool"))) -}}
        {{- $header = "\"]," -}}
    {{- end -}}
    {{- method .Method .Start $header }}{{.JSONName}}{{ method .Method $end "\"" -}}
    {{- if eq .FieldType "float32" -}}
        {{ method .Method "start" "" }}{{template "float32" .}}{{ method .Method "end" "" -}}
    {{- else if eq .FieldType "float64" -}}
        {{ method .Method "start" "" }}{{template "float64" .}}{{ method .Method "end" "" -}}
    {{- end -}}
    {{ if eq .End "lastend" -}}
        {{ method .Method "start" "" }}"}"{{ method .Method "end" "" -}}
    {{- end -}}
{{- end -}}

{{- define "parsefloat" -}}
    {{.Header}}{{.Start}}strconv.ParseFloat(float64({{.Value}}), 64){{.Stop}}{{.Footer}}
{{- end -}}

{{- define "float32" -}}
    strconv.FormatFloat(float64({{.Value}}), 'f', -1, 32)
{{- end -}}

{{- define "float64" -}}
    strconv.FormatFloat({{.Value}}, 'f', -1, 64)
{{- end -}}

{{define "floatarray"}}
    {{- $header := "\"," -}}
    {{- $end := "end" -}}
    {{- $final := "\"" -}}
    {{- if eq .Start "firststart" -}}
        {{- $end = "firstend" -}}
        {{- $header = "\"{" -}}
    {{- else if eq .PreviousType "string" -}}
        {{- $header = "\"\\\"," -}}
    {{- else if eq .PreviousType "[]string" -}}
        {{- $header = "\"\\\"]," -}}
    {{- else if (and (eq .PreviousArray "true") (not (eq .PreviousType "[]bool"))) -}}
        {{- $header = "\"]," -}}
    {{- end -}}
    {{ method .Method .Start $header }}{{.JSONName}}[{{ method .Method $end "\"" }}
    for i, v := range {{.Value}} {
        if i > 0 {
            {{ method .Method "start" "" }}","{{ method .Method "end" "" }}
        }
        {{ if eq .FieldType "[]float32" -}}
        {{ method .Method "start" "" }}{{template "float32" dict "Value" "v"}}{{ method .Method "end" "" -}}
        {{ else if eq .FieldType "[]float64" -}}
        {{ method .Method "start" "" }}{{template "float64" dict "Value" "v"}}{{ method .Method "end" "" -}}
        {{ end }}
    }
    {{ if eq .End "lastend" -}}
        {{ method .Method "start" "" }}"]}"{{ method .Method "end" "" }}
    {{- end -}}
{{end}}