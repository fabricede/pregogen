// {{- /* Go Template */ -}}.
{{- define "string" -}}
    {{- $header := "\"," -}}
    {{- $end := "end" -}}
    {{- if eq .Start "firststart" -}}
        {{- $end = "firstend" -}}
        {{- $header = "\"{" -}}
    {{- else if eq .PreviousType "string" -}}
        {{- $header = "\"\\\"," -}}
    {{- else if eq .PreviousType "[]string" -}}
        {{- $header = "\"\\\"]," -}}
    {{- else if (and (eq .PreviousArray "true") (not (eq .PreviousType "[]bool"))) -}}
        {{- $header = "\"]," -}}
    {{- end -}}
    {{- method .Method .Start $header }}{{.JSONName}}{{ method .Method $end "\\\"\"" -}}
    {{ method .Method "start" "" }}{{.Value}}{{ method .Method "end" "" -}}
    {{ if eq .End "lastend" -}}
        {{ method .Method "start" "" }}"\"}"{{ method .Method "end" "" }}
    {{- end -}}
{{- end -}}

{{define "stringarray"}}
    {{- $header := "\"," -}}
    {{- $end := "end" -}}
    {{- $final := "\"" -}}
    {{- if eq .Start "firststart" -}}
        {{- $end = "firstend" -}}
        {{- $header = "\"{" -}}
    {{- else if eq .PreviousType "string" -}}
        {{- $header = "\"\\\"," -}}
    {{- else if eq .PreviousType "[]string" -}}
        {{- $header = "\"\\\"]," -}}
    {{- else if (and (eq .PreviousArray "true") (not (eq .PreviousType "[]bool"))) -}}
        {{- $header = "\"]," -}}
    {{- end -}}
    {{ method .Method .Start $header }}{{.JSONName}}[\"{{ method .Method $end "\"" }}
    for i, v := range {{.Value}} {
        if i > 0 {
            {{ method .Method "start" "\"" }}\",\"{{ method .Method "end" "\"" }}
        }
        {{ method .Method "start" "" }}v{{ method .Method "end" "" -}}
    }
    {{ if eq .End "lastend" -}}
        {{ method .Method "start" "" }}"\"]}"{{ method .Method "end" "" -}}
    {{- end -}}
{{end}}